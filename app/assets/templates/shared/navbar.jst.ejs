<ul class="nav-logo">
  <li class="logo">
    <img src="meeplespacelogo"><br>
    <h1>meeple.space</h1>
  </li>
  <% if (Meeplespace.currentUser.isSignedIn()) { %>
    <% var currentCityId = Meeplespace.currentUser.get('city_id') %>
    <li class="nav-city-link">
      <a href="#cities/<%= currentCityId %>">
        <% Meeplespace.cities.getOrFetch(currentCityId).get('name') %>
        <% debugger %>
      </a>
    </li>
  <% } %>
</ul>
<ul class="nav-items">
  <li class="nav-item"><a href="#cities">Cities</a></li>
  <% if (Meeplespace.currentUser.isSignedIn()) { %>
    <li class="nav-item"><a href="#cities/<%= currentCityId %>">Let's Play Games!</a></li>
    <li class="nav-item"><a href="#users/<%= Meeplespace.currentUser.id %>">Account</a></li>
    <li class="nav-item sign-out"><a href="#">Sign Out</a></li>
  <% } else { %>
    <li class="nav-item"><a href="#users/new">Sign Up</a></li>
    <li class="nav-item"><a href="#session/new">Sign In</a></li>
  <% } %>
</ul>